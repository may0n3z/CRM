<Window x:Class="var11.klient"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:var11"
        mc:Ignorable="d"
        Title="Клиент" Height="450" Width="800">
    <Grid>
        <Button x:Name="buyAdvertisement" Content="Купить рекламу" HorizontalAlignment="Center" Height="40"  VerticalAlignment="Top" Width="220" Margin="0,100,0,0" Click="buyAdvertisement_Click"/>
        <Button x:Name="historyK" Content="История заказов" HorizontalAlignment="Center" Height="40"  VerticalAlignment="Top" Width="220" Margin="0,200,0,0" Click="historyK_Click"/>
        <Button x:Name="Back" Content="Назад" HorizontalAlignment="Center" Height="40"  VerticalAlignment="Top" Width="220" Margin="0,300,0,0" Click="Back_Click"/>
    </Grid>
</Window>
