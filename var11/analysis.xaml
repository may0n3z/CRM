<Window x:Class="var11.analysis"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:var11"
        mc:Ignorable="d"
        Title="analysis" Height="450" Width="800">
    <Grid>
        <Button x:Name="Back" Content="Назад" HorizontalAlignment="Center" Height="40" VerticalAlignment="Top" Width="220" Margin="0,330,0,0" Click="Button_Click"/>
        <Label Content="Общая сумма продаж" HorizontalAlignment="Center" Margin="0,71,0,0" VerticalAlignment="Top" Width="220" Height="25"/>
        <TextBox x:Name="FullMoney" HorizontalAlignment="Center" Margin="0,101,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="220" Height="20"/>
        <Button x:Name="Update" Content="Обновить" HorizontalAlignment="Center" Height="40" Margin="0,141,0,0" VerticalAlignment="Top" Width="220"/>
    </Grid>
</Window>
