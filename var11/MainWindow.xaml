<Window x:Class="var11.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:var11"
        mc:Ignorable="d"
        Title="Авторизация" Height="450" Width="800">
    <Grid>
        <Button x:Name="Entrance" Content="Войти" HorizontalAlignment="Center" Height="40"  VerticalAlignment="Top" Width="120" Margin="0,300,0,0" Click="Entrance_Click"/>
        <TextBox x:Name="Password" HorizontalAlignment="Center" Height="20" Margin="0,198,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="Name" HorizontalAlignment="Center" Height="20" Margin="0,145,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="120"/>
        <TextBlock HorizontalAlignment="Center" Height="20" Margin="0,125,0,0" TextWrapping="Wrap" Text="Логин:" VerticalAlignment="Top" Width="120"/>
        <TextBlock HorizontalAlignment="Center" Height="20" Margin="0,178,0,0" TextWrapping="Wrap" Text="Пароль:" VerticalAlignment="Top" Width="120"/>
        <Frame x:Name="frame" NavigationUIVisibility="Hidden"></Frame>
    </Grid>
</Window>
